datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Stock {
  id        Int      @id @default(autoincrement())
  name      String
  symbol    String   @unique
  price     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
model User {
  id          Int      @id @default(autoincrement())
  firebaseUid String   @unique
  email       String   @unique
  name        String?
  photoURL    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

